{
	"env": {
		"amd": false,
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"rules": {
		"accessor-pairs": 2,
		"array-bracket-spacing": [2, "never"],
		"array-callback-return": 2,
		"arrow-parens": [2, "as-needed"],
		"arrow-spacing": 2,
		"block-scoped-var": 2,
		"camelcase": [2, {
			"properties": "always"
		}],
		"capitalized-comments": [2, "always", {
			"ignorePattern": "webpack"
		}],
		"comma-dangle": [2, "never"],
		"comma-style": [2, "last"],
		"comma-spacing": 0,
		"complexity": 2,
		"consistent-return": 2,
		"consistent-this": 2,
		"curly": 2,
		"default-case": 2,
		"dot-notation": 2,
		"dot-location": [2, "property"],
		"eol-last": [2, "always"],
		"eqeqeq": 2,
		"function-paren-newline": [2, "consistent"],
		"guard-for-in": 2,
		"indent": [2, "tab", {
			/* TODO: Remove */
			"ignoredNodes": ["TemplateLiteral"],
			"SwitchCase": 1
		}],
		"jsx-quotes": [2, "prefer-double"],
		"key-spacing": [2, {
			"beforeColon": false,
			"afterColon": true,
			"mode": "strict"
		}],
		"keyword-spacing": 0,
		"linebreak-style": [2, "unix"],
		"multiline-ternary": 2,
		"new-parens": 2,
		"newline-per-chained-call": 2,
		"no-caller": 2,
		"no-case-declarations": 2,
		"no-cond-assign": 2,
		"no-console": ["off"],
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty": 2,
		"no-empty-character-class": 2,
		"no-empty-function": 2,
		"no-empty-pattern": 2,
		"no-eval": 2,
		"no-ex-assign": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-semi": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-func-assign": 2,
		"no-invalid-regexp": 2,
		"no-implicit-coercion": 2,
		"no-implicit-globals": 2,
		"no-implied-eval": 2,
		"no-iterator": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-multiple-empty-lines": [2, {
			"max": 1,
			"maxEOF": 1
		}],
		"no-negated-in-lhs": 2,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-obj-calls": 2,
		"no-octal": 2,
		"no-octal-escape": 2,
		"no-param-reassign": 2,
		"no-proto": 2,
		"no-redeclare": [2, {
			"builtinGlobals": true
		}],
		"no-regex-spaces": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-assign": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-sparse-arrays": 2,
		"no-this-before-super": 2,
		"no-throw-literal": 2,
		"no-trailing-spaces": 2,
		"no-underscore-dangle": 2,
		"no-unneeded-ternary": 2,
		"no-delete-var": 2,
		"no-unexpected-multiline": 2,
		"no-unmodified-loop-condition": 2,
		"no-unreachable": 2,
		"no-unused-expressions": 0,
		"no-useless-call": 2,
		"no-useless-concat": 2,
		"no-useless-escape": 2,
		"no-var": 2,
		"no-void": 2,
		"no-warning-comments": [2, {
			"terms": ["todo", "fixme", "fix", "bugfix"],
			"location": "anywhere"
		}],
		"no-with": 2,
		"object-curly-newline": [2, {
			"ExportDeclaration": {
				"minProperties": 3,
				"multiline": true
			},
			"ImportDeclaration": {
				"minProperties": 3,
				"multiline": true
			},
			"ObjectExpression": {
				"minProperties": 1,
				"multiline": true
			},
			"ObjectPattern": {
				"minProperties": 2,
				"multiline": true
			}
		}],
		"object-curly-spacing": [2, "always"],
		"object-property-newline": 2,
		"object-shorthand": [2, "always", {
			"avoidQuotes": true
		}],
		"operator-linebreak": [2, "before"],
		"prefer-arrow-callback": 2,
		"prefer-const": 2,
		"quotes": [2, "double", {
			"allowTemplateLiterals": true
		}],
		"radix": [2, "as-needed"],
		"semi": [2, "always"],
		"sort-keys": [2, "asc", {
			"caseSensitive": false,
			"natural": true
		}],
		"spaced-comment": [2, "always", {
			"block": {
				"balanced": true
			}
		}],
		"strict": [2, "global"],
		"template-curly-spacing": "off",
		"yoda": [2, "never"]
	}
}
