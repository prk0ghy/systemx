	"dependencies": {
		"formik": "^2.2.9",
		"hyphenated": "^1.2.0",
		"hyphenated-de": "^1.2.0",
		"i18n-iso-countries": "^6.8.0",
		"next": "11.1.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-tracked": "^1.7.3"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.15.0",
		"@next/eslint-plugin-next": "^11.1.0",
		"autoprefixer": "^10.3.1",
		"cssnano": "^5.0.7",
		"eslint": "7.32.0",
		"eslint-config-next": "11.1.0",
		"get-google-fonts": "^1.2.2",
		"postcss": "^8.3.6",
