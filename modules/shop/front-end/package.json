	},
	"name": "front-end",
	"private": true,
	"scripts": {
		"build": "npm run download:fonts && next build",
		"dev": "npm run download:fonts:if-necessary; next dev",
		"download:fonts": "get-google-fonts -i \"https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700,800,900&display=swap\" -wo .cache/fonts -p ../.cache/fonts/",
		"download:fonts:if-necessary": "[ ! -d .cache/fonts ] && npm run download:fonts",
		"lint": "next lint",
		"start": "next start"
	},
	"version": "0.0.1"
}
