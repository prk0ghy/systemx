{
	"env": {
		"browser": true,
		"es2015": true
	},
	"extends": [
		"next/core-web-vitals",
		"eslint:recommended",
		"./.eslintrc.base.json",
		"./.eslintrc.react.json"
	],
	"rules": {
		"@next/next/no-img-element": "off",
		"sort-keys": "off",
		"capitalized-comments": "off",
		"no-multi-spaces": "off",
		"no-multiple-empty-lines": "off",
		"no-inline-comments": "off",
		"object-property-newline": "off",
		"object-curly-newline": "off"
	}
}
